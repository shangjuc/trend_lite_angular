{
  "name": "trend-lite-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "cross-env NODE_ENV=development && ng serve ",
    "build": "cross-env NODE_ENV=production && ng build --base-href /trend_lite_ng2/  --deploy-url /trend_lite_ng2/assets/ --output-path ./dist/static ",
    "build2": "cross-env NODE_ENV=production && ng build --base-href /trend_lite_ng2/ --deploy-url /trend_lite_ng2/assets/ --output-path ../analytics_gae/static/trend_lite_ng2_dist/static ",

    "ah_server": "angular-http-server -p 9000 --base-href trend-lite-ng2 --path ./dist/static",
    "live_server": "cd ./dist/static && live-server --port=9000 --entry-file=./index.html",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "^13.2.2",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/material": "^13.2.2",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "angular-plotly.js": "^4.0.4",
    "plotly.js-dist-min": "^2.9.0",
    "postcss": "^8.4.6",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "updated": "^2.0.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular/cli": "^13.1.4",
    "@angular/compiler-cli": "~13.1.0",
    "@ngrx/schematics": "^13.0.2",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@types/plotly.js-dist-min": "^2.3.0",
    "cross-env": "^7.0.3",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "tailwindcss": "^3.0.16",
    "typescript": "~4.5.2"
  }
